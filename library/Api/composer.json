    "homepage": "https://github.com/library/api",
    "keywords": ["Laravel", "Api"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Library\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Library\\Api\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Library\\Api\\ApiServiceProvider"
            ],
            "aliases": {
                "Api": "Library\\Api\\Facades\\Api"
            }
        }
    }
}
